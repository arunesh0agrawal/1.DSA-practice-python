// Online C++ compiler to run C++ program online
#include <iostream>
using namespace std;
int main()
{

for(int i=6;i>0;i--)
{
    for(int j=6;j>6-i+1;j--)
    {
        if(i==6)
        {
            cout<<"*";
          
        }
        else
        {
        if(j==6)
        {
            cout<<"*";
        }
        else
        {
            cout<<" ";
        }
        }
        
    }
    cout<<"*";
    cout<<endl;
}
}
